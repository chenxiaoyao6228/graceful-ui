    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-less": "^1.0.0",
    "prettier": "^2.0.4",
    "pretty-quick": "^2.0.1",
    "style-loader": "^1.1.3",
    "vue": "^2.6.11",
    "vue-jest": "^3.0.5",
    "vue-loader": "^15.9.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2"
